    "import matplotlib.pyplot as plt\n",
    "\n",
    "\n",
    "def on_close(event):\n",
    "    print('Closed Figure!')\n",
    "\n",
    "fig = plt.figure()\n",
    "fig.canvas.mpl_connect('close_event', on_close)\n",
    "\n",
    "plt.text(0.35, 0.5, 'Close Me!', dict(size=30))\n",
    "plt.show()"
   ]
